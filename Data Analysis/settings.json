{
    "serviceInfo": {
        "serviceID": "DataAnalysis",
        "serviceName": "Data Analysis Service",
        "serviceDescription": "Processes raw sensor data and generates statistical insights and trends for visualization and decision making.",
        "serviceType": "analytics_service",
        "version": "1.0.0",
        "endpoints": [
            "REST: GET /analyze/{device_id}?period={duration}&metrics={list}",
            "REST: GET /trends/{device_id}?period={duration}",
            "REST: GET /patterns/{device_id}?type={usage|temperature|efficiency}",
            "REST: GET /health",
            "REST: GET /status"
        ]
    },
    "catalog": {
        "url": "http://catalog:8001",
        "registration_interval_seconds": 300,
        "ping_interval_seconds": 60
    },
    "influxdb_adaptor": {
        "base_url": "http://influxdb_adaptor:8002",
        "timeout_seconds": 15
    },
    "analysis": {
        "supported_periods": ["1h", "6h", "12h", "1d", "7d", "1m", "3m"],
        "cache_duration_minutes": 10,
        "min_data_points_required": 20,
        "temperature_optimal_range": [2, 6]
    },
    "defaults": {
        "default_period": "24h",
        "decimal_precision": 2
    },
    "lastUpdate": "2025-09-06T14:30:00Z",
    "configVersion": 1
}