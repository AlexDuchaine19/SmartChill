{
  "serviceInfo": {
    "serviceID": "EnergyOptimization",
    "serviceName": "SmartChill Energy Optimization",
    "serviceDescription": "Energy optimization service using compressor duty cycle analysis and runtime prediction",
    "serviceType": "optimization",
    "version": "3.1.0",
    "endpoints": [
      "REST: GET /health",
      "REST: GET /status",
      "REST: GET /optimize/{device_id}",
      "REST: GET /predictions/{device_id}",
      "REST: GET /runtime/{device_id}"
    ]
  },
  "catalog": {
    "url": "http://catalog:8001",
    "registration_interval_seconds": 300
  },
  "influxdb_adaptor": {
    "base_url": "http://influxdb_adaptor:8002",
    "timeout_seconds": 15
  },
  "ml": {
    "enable_predictions": true,
    "runtime_features": [
      "avg_temperature",
      "temperature_variance",
      "stability_score",
      "daily_openings",
      "avg_door_duration",
      "hour_factor",
      "day_factor"
    ],
    "min_training_samples": 20
  },
  "defaults": {
    "fallback_power_specs": {
      "compressor_power_watts": 120,
      "base_power_watts": 120,
      "recovery_time_multiplier": 1.5,
      "max_efficient_openings_per_day": 15,
      "optimal_duty_cycle_range": [0.25, 0.45],
      "min_cycle_duration_minutes": 8,
      "max_cycle_duration_minutes": 45
    }
  },
  "configVersion": 2,
  "lastUpdate": "2025-10-22T19:00:00Z"
}