{
    "serviceInfo": {
        "serviceID": "FridgeStatusControl",
        "serviceName": "Fridge Status Control Service",
        "serviceDescription": "Monitors fridge temperature and humidity via MQTT to detect malfunctions and send malfunction alerts when conditions are abnormal.",
        "serviceType": "monitoring_service",
        "version": "1.0.0",
        "endpoints": [
            "MQTT Subscribe: Group17/SmartChill/Devices/+/+/temperature",
            "MQTT Subscribe: Group17/SmartChill/Devices/+/+/humidity",
            "MQTT Subscribe: Group17/SmartChill/FridgeStatusControl/+/config_update",
            "MQTT Publish: Group17/SmartChill/+/Alerts/Malfunction",
            "MQTT Publish: Group17/SmartChill/FridgeStatusControl/+/config_ack",
            "MQTT Publish: Group17/SmartChill/FridgeStatusControl/+/config_error",
            "MQTT Publish: Group17/SmartChill/FridgeStatusControl/+/config_data"
        ]
    },
    "catalog": {
        "url": "http://catalog:8001",
        "registration_interval_seconds": 300,
        "ping_interval_seconds": 60
